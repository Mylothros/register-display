<web-app>
  <display-name>My Web Application</display-name>

  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.display_user.register</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/api</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>displayUsers</servlet-name>
    <servlet-class>com.display_user.displayUsers</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>displayUsers</servlet-name>
    <url-pattern>/api2</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>homepage.jsp</welcome-file>
  </welcome-file-list>
</web-app>